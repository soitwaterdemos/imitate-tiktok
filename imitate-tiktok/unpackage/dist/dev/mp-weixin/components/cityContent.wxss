
.city-content {
  width: 100%;
  height: 100%;
  background: red;
}
.position {
  width:100%;
  height: 40px;
  padding-top: 60px;
  color: #999999;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-flow: row nowrap;
          flex-flow: row nowrap;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.position .left {
  font-size: 14px;
  -webkit-transform: translateX(10px);
          transform: translateX(10px);
}
.position .right {
  font-size: 12px;
  -webkit-transform: translateX(-10px);
          transform: translateX(-10px);
}
.video-list {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-flow: row wrap;
          flex-flow: row wrap;
  background: #000000;
}
.video-list .item {
  width: 50%;
  height: 300px;
  position: relative;
  /* align-items: center; */
}
.video-list .item .video {
  width: 92%;
  height: 100%;
  -webkit-transform: translateX(4%);
          transform: translateX(4%);
  background: yellow;
  z-index:18;
}
.video-list .item .img {
  position: absolute;
  bottom: 5px;
  right: 5px;
}
.video-list .item .img .picture {
  width: 30px;
  height: 30px;
  border-radius: 50%;
}

